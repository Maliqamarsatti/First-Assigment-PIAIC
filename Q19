console.log("Hey! " + invitationList[1] + message);
console.log("Hey! " + invitationList[2] + message);
console.log("Hey! " + invitationList[3] + message);

//Printing number of guest invited to dinner.
console.log("Number of people invited are " + invitationList.length);
